{
  "name": "sitekol-backend",
  "version": "1.0.0",
  "description": "SITEKOL Backend API - Metal Trading Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev-simple": "nodemon src/simple-server.ts",
    "dev-db": "nodemon src/server-with-db.ts",
    "test-db": "ts-node src/test-db.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:system": "ts-node src/test-system.ts",
    "test:api": "ts-node src/test-api-connection.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "metals",
    "trading",
    "api",
    "typescript",
    "express"
  ],
  "author": "Gabriel",
  "license": "MIT",
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.5.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^3.0.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "sharp": "^0.32.5",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/nodemailer": "^6.4.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
